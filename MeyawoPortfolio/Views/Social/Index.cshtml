@model List<MeyawoPortfolio.Models.TblSocialMedia>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/_Layoout.cshtml";
}

<h2>Sosyal Medya</h2> 
<table class="table table-bordered">
    <tr>
        <th>Sosyal Medya ID</th>
        <th>Başlık</th>
        <th>Sosyal Medya Url</th>
        <th>Sosyal Icon</th> 
        <th>Güncelle</th> 
        <th>Sil</th> 
    </tr> 
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.SocialMediaID</td>
            <td>@item.Tıtle</td>
            <td>@item.SocialMediaUrl</td>
            <td>@item.Icon</td> 

            <td><a href="/Social/UpdateSocial/@item.SocialMediaID" class="btn btn-success">Güncelle</a></td>
            <td><a href="/Social/DeleteSocial/@item.SocialMediaID" class="btn btn-danger">Sil</a></td>
        </tr>
    }
</table> 
<td><a href="/Social/CreateSocial/" class="btn btn-primary">Yeni Kayıt</a></td>

